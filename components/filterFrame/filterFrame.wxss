.filterFrame {
  height: 100rpx;
  width: 100%;
  border-bottom: 2rpx solid #cccccc;
  padding: 0 30rpx;
  position: relative;
  background-color: #fff;
}
.filterFrame .filterTitle {
  width: 100%;
  height: 100%;
}
.filterFrame .filterTitle ul {
  width: 100%;
  height: 100%;
  display: block;
}
.filterFrame .filterTitle ul .mainColor {
  color: #f96771;
}
.filterFrame .filterTitle ul li {
  width: 23%;
  float: left;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.filterFrame .filterTitle ul li:last-child {
  width: 30rpx;
  float: right;
}
.filterFrame .filterTitle ul li text {
  flex: 1;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}
.filterFrame .filterTitle ul li .icon_paixu_false,
.filterFrame .filterTitle ul li .icon_paixu_true {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  float: right;
}
.filterFrame .filterTitle ul li .icon_xiajiantou_false,
.filterFrame .filterTitle ul li .icon_xiajiantou_true {
  width: 20rpx;
  height: 20rpx;
}

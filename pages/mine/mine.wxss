.mine {
  position: relative;
}
.mine .headerBar {
  background-color: #f9677121;
  height: 200rpx;
  display: flex;
  justify-content: center;
}
.mine .headerBar .headPortrait {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  float: left;
}
.mine .headerBar .userName {
  float: left;
  height: 100%;
  line-height: 200rpx;
  padding-left: 40rpx;
  font-size: 50rpx;
}
.mine .list {
  width: 100%;
  height: 200rpx;
  margin-top: 20rpx;
  background-color: #f9677121;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mine .list .icon {
  margin-right: 40rpx;
}
.mine .list view {
  margin-left: 40rpx;
  font-size: 50rpx;
}

.houseFrontCoverOuter {
  width: 100%;
  padding: 40rpx 40rpx 0 40rpx;
}
.houseFrontCoverOuter .houseFrontCover {
  width: 100%;
  height: 400rpx;
  background-color: #686767;
  color: #ffffff;
  position: relative;
  text-align: center;
  line-height: 260rpx;
  border-radius: 20rpx;
}
.houseFrontCoverOuter .houseFrontCover .upLoad-before .houseFrontCoverTitle {
  top: 45%;
  left: 50%;
  text-align: center;
  line-height: 320rpx;
  transform: translate(-50%, -50%);
  width: 260rpx;
  height: 260rpx;
  border: 2rpx solid #eee;
  border-radius: 50%;
  position: absolute;
  background-color: #777;
}
.houseFrontCoverOuter .houseFrontCover .upLoad-before .houseFrontCoverTitle .icon {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 40rpx;
  left: 76rpx;
}
.houseFrontCoverOuter .houseFrontCover .upLoad-before .additionalTitle {
  width: 100%;
  height: 30px;
  background-color: rgba(204, 204, 204, 0.459);
  text-align: center;
  line-height: 30px;
  position: absolute;
  bottom: 0;
  z-index: 3;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.houseFrontCoverOuter .houseFrontCover image {
  width: 100%;
  height: 100%;
  border-radius: 10px;
  position: absolute;
  z-index: 2;
  left: 0;
  top: 0;
}
.houseFrontCoverOuter .houseOtherImg {
  padding-top: 32rpx;
  width: 100%;
}
.houseFrontCoverOuter .houseOtherImg ul {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.houseFrontCoverOuter .houseOtherImg ul li {
  width: 140rpx;
  height: 220rpx;
  display: block;
  font-size: 32rpx;
  color: #666;
  position: relative;
}
.houseFrontCoverOuter .houseOtherImg ul li .otherImg {
  width: 140rpx;
  height: 140rpx;
  border-radius: 20rpx;
  display: inline-block;
  overflow: hidden;
  background-color: #d4d4d4;
}
.houseFrontCoverOuter .houseOtherImg ul li .otherImg .icon {
  width: 100rpx;
  height: 96rpx;
  position: absolute;
  top: 22rpx;
  left: 22rpx;
}
.houseFrontCoverOuter .houseOtherImg ul li .otherImgInfo {
  display: block;
  text-align: center;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.houseFrontCoverOuter .houseOtherImg ul li .cancel {
  position: absolute;
  right: -16rpx;
  top: -14rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: red;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  font-size: 44rpx;
}
.houseFrontCoverAttentionOuter {
  width: 100%;
}
.houseFrontCoverAttentionOuter ul {
  width: 100%;
  background-color: #fff;
  overflow: hidden;
  display: block;
  padding-bottom: 20rpx;
}
.houseFrontCoverAttentionOuter ul li {
  display: block;
  line-height: 60rpx;
  margin-top: 10px;
  height: 60rpx;
  padding-left: 20rpx;
}
.houseFrontCoverAttentionOuter ul li text {
  width: 8rpx;
  height: 60rpx;
  background-color: red;
  float: left;
  margin-right: 20rpx;
}
